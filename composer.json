{
  "name": "plaisio/login-requirement-oauth2-league",
  "description": "PhpPlaisio: OAuth2 Login Requirement",
  "keywords": [
    "PhpPlaisio",
    "OAuth2",
    "Login Requirement"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "league/oauth2-client": "^2.0.0",
    "plaisio/exception": "^1.0",
    "plaisio/kernel": "^1.0",
    "plaisio/login-requirement": "^1.0",
    "plaisio/response-core": "^1.0"
  },
  "require-dev": {
    "phing/phing": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Login\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Login\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
    ]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
