{
  "name": "plaisio/login-requirement-oauth2-league",
  "description": "PhpPlaisio: OAuth2 Login Requirement",
  "keywords": [
    "PhpPlaisio",
    "OAuth2",
    "Login Requirement"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "league/oauth2-client": "^2.6.1",
    "plaisio/cgi": "^1.3.0",
    "plaisio/exception": "^1.2.0",
    "plaisio/kernel": "^3.2.1",
    "plaisio/login-requirement": "^1.2.0",
    "plaisio/response-core": "^2.8.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "plaisio/console-kernel": "^1.0.1"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Login\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Login\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
    ]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
